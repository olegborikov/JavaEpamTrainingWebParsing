<?xml version="1.0" encoding="UTF-8"?>
<greenhouse xmlns="http://www.example.com/greenhouse">
    <decorative-flower name="rose" soil="unpaved">
        <visual-parameters>
            <stem-color>white</stem-color>
            <leave-color>red</leave-color>
            <average-plant-size>40</average-plant-size>
        </visual-parameters>
        <multiplying>cuttings</multiplying>
        <growing-tips>
            <temperature>20</temperature>
            <need-lighting>true</need-lighting>
            <watering-per-week>100</watering-per-week>
        </growing-tips>
    </decorative-flower>

    <wild-flower name="thrift">
        <visual-parameters>
            <stem-color>white</stem-color>
            <leave-color>pink</leave-color>
            <average-plant-size>25</average-plant-size>
        </visual-parameters>
        <multiplying>seeds</multiplying>
        <origin>10</origin>
    </wild-flower>
</greenhouse>
